<% provide(:title,"All Users")%>
<div class="page-header">
  <h2>All Users</h2>
</div>
<%= will_paginate %>
  <ul class="users list-group">
    <% @users.each do |user| %>
    <li class="list-group-item">
       <%= gravatar_for user %>
      <%= link_to user.name.capitalize,user %>
    </li>
    <% end %>
  </ul>
<%= will_paginate %>
